{
	"name": "pokecode",
	"displayName": "pokecode",
	"description": "Play Pokemon background music as you code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.57.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:pokecode.background"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pokecode.background",
				"title": "Pokécode: Play Background Music"
			},
			{
				"command": "pokecode.stop",
				"title": "Pokécode: Stop Background Music"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.57.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"play-sound": "^1.1.3"
	}
}
